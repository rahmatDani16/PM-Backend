
<div class="card">
    <div class="card-header">
      <h3 class="card-title">Tabel Data Masantri</h3>
    </div>
    <div class="card-body p-0">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <table id="tableMasantri" class="table table-striped">
              <table border="1" cellspacing="0" cellpadding="5">
                <thead>
                  <tr>
                    <th>Id</th>
                    <th>Nama</th>
                    <th>Nim</th>
                    <th>Prodi</th>
                  </tr>
                </thead>
                <tbody>
                  <% dataMahasantri.forEach(function(mahasantri) { %>
                    <tr>
                      <td><%= mahasantri.id %></td>
                      <td><%= mahasantri.nama %></td>
                      <td><%= mahasantri.nim %></td>
                      <td><%= mahasantri.prodi %></td>
                    </tr>
                  <% }); %>
                </tbody>
              </table>
              <tbody>
                <!-- Data akan diisi melalui JavaScript -->
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
